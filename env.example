# Application Configuration
NODE_ENV=development
LOG_LEVEL=info
MAX_RETRY_COUNT=3

# Database Configuration
SQL_CONNECTION_STRING=Server=localhost;Database=fuma_tab;User Id=sa;Password=your_password;TrustServerCertificate=true;

# RabbitMQ Configuration
RABBITMQ_URL=amqp://admin:admin123@localhost:5672
RABBITMQ_QUEUE=tab-integration-queue
RABBITMQ_EXCHANGE=fuma-tab-exchange
RABBITMQ_ROUTING_KEY=tab-integration
RABBITMQ_PREFETCH_COUNT=1

# PiqSoft API Configuration
PIQSOFT_BASE_URL=https://api.piqsoft.com
PIQSOFT_API_KEY=your_piqsoft_api_key_here
PIQSOFT_TIMEOUT=30000
